<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bindings version="1.0"
              xmlns="http://java.sun.com/xml/ns/jaxb"
              xmlns:xsd="http://www.w3.org/2001/XMLSchema"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns:namespace="http://jaxb2-commons.dev.java.net/namespace-prefix"
              xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd
              http://jaxb2-commons.dev.java.net/namespace-prefix http://java.net/projects/jaxb2-commons/sources/svn/content/namespace-prefix/trunk/src/main/resources/prefix-namespace-schema.xsd">
    <bindings
            schemaLocation="maven:org.springframework:spring-beans:::4.1.9.RELEASE!/org/springframework/beans/factory/xml/spring-beans-4.1.xsd">
        <bindings node="//xsd:element[@name='ref']">
            <property name="beanRef"/>
        </bindings>
        <bindings node="//xsd:element[@name='value']">
            <property name="beanValue"/>
        </bindings>
        <bindings node="//xsd:element[@name='key']">
            <property name="beanKey"/>
        </bindings>
        <bindings>
            <namespace:prexix name="bean"/>
        </bindings>
    </bindings>
</bindings>