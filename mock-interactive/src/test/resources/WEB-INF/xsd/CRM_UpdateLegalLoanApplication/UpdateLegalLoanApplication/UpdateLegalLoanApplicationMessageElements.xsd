			<documentation>Тело сообщения</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element ref="ullarq:UpdateLegalLoanApplicationRq" minOccurs="0"/>
				<element ref="ullars:UpdateLegalLoanApplicationRs" minOccurs="0"/>			
