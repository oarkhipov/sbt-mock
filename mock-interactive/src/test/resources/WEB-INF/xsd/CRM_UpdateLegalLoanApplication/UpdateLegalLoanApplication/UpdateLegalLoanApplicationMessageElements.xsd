<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
		xmlns:tns="http://sbrf.ru/NCPCA/CRM/UpdateLegalLoanApplicationMessageElements/"
		xmlns:hdr="http://sbrf.ru/ESB/mq/headers" 
