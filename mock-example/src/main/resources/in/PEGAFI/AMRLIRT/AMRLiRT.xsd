                <xsd:annotation>
                    <xsd:documentation>Уникальный идентификатор запроса</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RqTm" type="DateTime" minOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Дата и время передачи сообщения</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SPName" type="SPName_Type" minOccurs="1">
