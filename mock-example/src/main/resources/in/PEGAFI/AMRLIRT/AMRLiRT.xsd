        <xsd:restriction base="C">
            <xsd:maxLength value="15"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Code_Type">
        <xsd:annotation>
            <xsd:documentation>Код</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="30"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="SystemId_Type">
        <xsd:annotation>
            <xsd:documentation>Идентификатор системы, которой адресовано сообщение</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="50"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="NumValue_Type">
        <xsd:annotation>
            <xsd:documentation>Числовое значение показателя</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="30"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Group_Type">
        <xsd:annotation>
            <xsd:documentation>Группа показателя</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="30"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="FactorIntegrationId_Type">
        <xsd:annotation>
            <xsd:documentation>Код показателя, единый для двух систем</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="30"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Factor_Type">
        <xsd:annotation>
            <xsd:documentation>Порядковый номер параметра</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="255"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Order_Type">
        <xsd:annotation>
            <xsd:documentation>Порядковый номер параметра</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="255"/>
            <xsd:pattern value="^(-?\d+(,\d+)?)?$"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Type_Type">
        <xsd:annotation>
            <xsd:documentation>Тип рейтинга</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="30"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Value_Type">
        <xsd:annotation>
            <xsd:documentation>Значение рейтинга</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="255"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Name_Type">
        <xsd:annotation>
            <xsd:documentation>Наименование рассчитанного рейтинга</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="255"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="IsPrimary_Type">
        <xsd:annotation>
            <xsd:documentation>Признак основной</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="RbBrchType">
        <xsd:annotation>
            <xsd:documentation>Тип данных для 6-значного кода территориального банка. Используется для передачи кода ТБ, в котором обслуживается продукт клиента</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:length value="6"/>
            <xsd:pattern value="[0-9]{6}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="RbTbBrchType">
        <xsd:annotation>
            <xsd:documentation>Тип данных для 8-значного кода территориального банка. Используется для передачи кода ТБ, в котором открыт счет МБК, по которой клиент произвел идентификацию на УС</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:length value="8"/>
            <xsd:pattern value="[0-9]{8}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="IssuedBy_Type">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="255"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="IdNum_Type">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="12"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="IdSeries_Type">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="12"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="IdType_Type">
        <xsd:annotation>
            <xsd:documentation>Тип документов, удостоверяющих личность</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="4"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Gender_Type">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
    <xsd:simpleType name="TaxId_Type">
        <xsd:annotation>
            <xsd:documentation>Тип данных "ИНН".Элемент может содержать 10 или 12 символов</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="40"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ServerStatusCode_Type">
        <xsd:annotation>
            <xsd:documentation>
                Статусный код сервера. Это значение выдается пользователю на экран и позволяет провайдеру лучше диагностировать ошибку.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="20"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="StatusCode_Type">
        <xsd:annotation>
            <xsd:documentation>
                Статусный код возврата.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="Long"/>
    </xsd:simpleType>
    <xsd:simpleType name="StatusDesc_Type">
        <xsd:annotation>
            <xsd:documentation>
                Описание статуса.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="255"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="OperUID_Type">
        <xsd:annotation>
            <xsd:documentation>Уникальный идентификатор операции</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="UUID"/>
    </xsd:simpleType>
    <xsd:simpleType name="RqUID_Type">
        <xsd:annotation>
            <xsd:documentation>
                Идентификатор запроса.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="32"/>
            <xsd:pattern value="([0-9]|[a-f]|[A-F]){32}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="SPName_Type">
        <xsd:annotation>
            <xsd:documentation>
                Система-иницатор
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="Identifier"/>
    </xsd:simpleType>
    <xsd:simpleType name="Summa_Type">
        <xsd:annotation>
            <xsd:documentation>
                Сумма
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="100"/>
            <xsd:pattern value="(\+|\-)?[0-9]*\.?[0-9]*"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="RegionId_Type">
        <xsd:annotation>
            <xsd:documentation>
                Номер ТБ
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="3"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="AgencyId_Type">
        <xsd:annotation>
            <xsd:documentation>Номер ОСБ</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="4"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="BranchDescription_Type">
        <xsd:annotation>
            <xsd:documentation>Описание подразделения</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="1024"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="BranchId_Type">
        <xsd:annotation>
            <xsd:documentation>Номер ВСП</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="5"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="FullName_Type">
        <xsd:annotation>
            <xsd:documentation>
                ФИО
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="1024"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="LastName_Type">
        <xsd:annotation>
            <xsd:documentation>
                Фамилия.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="60"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="FirstName_Type">
        <xsd:annotation>
            <xsd:documentation>Имя пользователя. Имя в составе ФИО.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="60"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="MiddleName_Type">
        <xsd:annotation>
            <xsd:documentation>Отчество.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="60"/>
        </xsd:restriction>
    </xsd:simpleType>
    <!-- Строковые типы данных -->
    <xsd:simpleType name="C">
        <xsd:annotation>
            <xsd:documentation>Строковый тип с поддержкой русского языка в кодировке UTF-8.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
    <xsd:simpleType name="NC">
        <xsd:annotation>
            <xsd:documentation>Строковый тип в кодировке ISO Latin-1.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
    <xsd:simpleType name="Identifier">
        <xsd:annotation>
            <xsd:documentation>Идентификатор.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="NC">
            <xsd:maxLength value="36"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="UUID">
        <xsd:annotation>
            <xsd:documentation>Уникальный 16 байтовый идентификатор. Для обмена в рамках интеграционных проектов Сбербанка представляется набором из 32 символов без разделителей в шестнадцатиричной системе.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="NC">
            <xsd:maxLength value="32"/>
            <xsd:pattern value="([0-9]|[a-f]|[A-F]){32}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <!-- Конец строковых типов данных -->
    <!-- Логические типы данных -->
    <xsd:simpleType name="Boolean">
        <xsd:annotation>
            <xsd:documentation>Логический тип.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:boolean"/>
    </xsd:simpleType>
    <!-- Конец логических типов данных -->
    <!-- Числовые типы данных -->
    <xsd:simpleType name="Integer">
        <xsd:annotation>
            <xsd:documentation>Целое число.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer"/>
    </xsd:simpleType>
    <xsd:simpleType name="Long">
        <xsd:annotation>
            <xsd:documentation>Длинное число.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:long"/>
    </xsd:simpleType>
    <xsd:simpleType name="Decimal">
        <xsd:annotation>
            <xsd:documentation>Десятичный тип.	</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:decimal">
            <xsd:pattern value="(\+|\-)?[0-9]*\.?[0-9]*"/>
        </xsd:restriction>
    </xsd:simpleType>
    <!-- Конец числовых типов данных -->
    <!-- Дата и время -->
    <xsd:simpleType name="Date">
        <xsd:annotation>
            <xsd:documentation>Дата</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:date"/>
    </xsd:simpleType>
    <xsd:simpleType name="DateTime">
        <xsd:annotation>
            <xsd:documentation>Дата и время</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:dateTime"/>
    </xsd:simpleType>
    <xsd:simpleType name="Timestamp">
        <xsd:annotation>
            <xsd:documentation>Отметка времени</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:dateTime"/>
    </xsd:simpleType>
    <xsd:simpleType name="Time">
        <xsd:annotation>
            <xsd:documentation>Время</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:time"/>
    </xsd:simpleType>
    <!-- Конец даты и времени -->
    <!-- Перечислимые типы -->
    <xsd:simpleType name="ClosedEnum">
        <xsd:annotation>
            <xsd:documentation>Закрытое перечисление. Перечисление содержит	конечный набор допустимых значений. Другие значения не допустимы. Обнаружении значения,	не входящего	в набор допустимых значений,	трактуется как ошибка в любом узле				системы на любом этапе обработки
                сообщения.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="80"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="OpenEnum">
        <xsd:annotation>
            <xsd:documentation>Открытое перечисление. Перечисление содержит конечный набор рекомендуемых значений. Допустимы значения, не входящие в рекомендуемый набор. Интепретация корректности значения осуществляется автором и конечным получателем. Промежуточные узлы не осуществляют проверку и считают значение всегда корректным.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="C">
            <xsd:maxLength value="80"/>
        </xsd:restriction>
    </xsd:simpleType>
    <!-- Конец перечислимых данных -->
    <!-- Бинарные типы данных -->
    <xsd:complexType name="Binary">
        <xsd:annotation>
            <xsd:documentation>Агрегат для хранения бинарных данных.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ContentType" type="ContentType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Тип данных.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BinLength" type="BinLength">
                <xsd:annotation>
                    <xsd:documentation>Длина данных.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BinData" type="BinData">
                <xsd:annotation>
                    <xsd:documentation>Бинарные данные.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="ContentType">
        <xsd:annotation>
            <xsd:documentation>Открытое перечисление. Соответствует стандарту IETF RFC 2046.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="OpenEnum"/>
    </xsd:simpleType>
    <xsd:simpleType name="BinData">
        <xsd:annotation>
            <xsd:documentation>Бинарные данные.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:base64Binary"/>
    </xsd:simpleType>
    <xsd:simpleType name="BinLength">
        <xsd:annotation>
            <xsd:documentation>Длина бинарных данных.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="Long"/>
    </xsd:simpleType>
    <xsd:simpleType name="ServerStatusDesc_Type">
        <xsd:annotation>
            <xsd:documentation>Исходное описание ошибки из внешней системы (заполняется только при значении StatusCode, отличном от 0.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
</xsd:schema>
