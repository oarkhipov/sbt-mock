            </xsd:element>
            <xsd:element name="RatingPeriodRec" type="RatingPeriodRec_Type" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Атрибуты периодов в разрезе которых заданы количественные показатели</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RatingSupportRec" type="RatingSupportRec_Type" minOccurs="0" maxOccurs="unbounded">
