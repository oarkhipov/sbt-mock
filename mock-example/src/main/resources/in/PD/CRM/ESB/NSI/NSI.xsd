		<xs:annotation>
			<xs:documentation>Запрос на активацию справочников</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TransferReferenceNf" type="TransferReferenceNf_Type"/>
	<xs:element name="TransferReferenceRq" type="TransferReferenceRq_Type">
		<xs:annotation>
			<xs:documentation>Передача справочников</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FieldNameValue">
