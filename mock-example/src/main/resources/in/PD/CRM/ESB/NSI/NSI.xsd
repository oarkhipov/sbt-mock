			<xs:documentation>Элемент списка атрибутов справочника</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FieldName" type="C30">
				<xs:annotation>
					<xs:documentation>Название (код) атрибута</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FieldValue" type="C45">
				<xs:annotation>
					<xs:documentation>Значение (ключ) атрибута</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FieldNameValue_Big">
		<xs:annotation>
			<xs:documentation>Элемент списка атрибутов справочника</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FieldName" type="C30">
				<xs:annotation>
					<xs:documentation>Название (код) атрибута</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FieldValue" type="C1024">
				<xs:annotation>
					<xs:documentation>Значение (ключ) атрибута</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Initiate_Package_Type">
		<xs:sequence/>
	</xs:complexType>
	<xs:complexType name="InitiateReferenceRq_Type">
		<xs:sequence>
			<xs:element name="MasterData" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Элемент основных данных справочника</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RefType" type="C30">
							<xs:annotation>
								<!-- TODO сделать ENUM? -->
								<xs:documentation>Код справочника                   </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="RefVersion" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Версия справочника</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="C">
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="RefUpdateType">
							<xs:annotation>
								<xs:documentation>Тип выгрузки по справочнику (полная версия, только изменения по сравнению с предыдущей версией справочника)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="ClosedEnum">
									<xs:enumeration value="Full"/>
									<xs:enumeration value="Change"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InitiateReferenceRs_Type">
		<xs:sequence>
			<xs:element name="Status" type="Status_Type"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NotifyReferenceActiveNf_Type">
		<xs:sequence>
			<xs:element name="Status" type="Status_Type"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NotifyReferenceActiveRq_Type">
		<xs:sequence>
			<xs:element name="MasterDataType" minOccurs="0">
