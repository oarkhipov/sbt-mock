<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
	xmlns:tns="http://sbrf.ru/ESB/mq/common"
		xmlns:hdr="http://sbrf.ru/ESB/mq/headers" 
		xmlns:canclsarq="http://sbrf.ru/NCPCA/LD/CancelLegalServiceApplicationRq/" 
		xmlns:canclsars="http://sbrf.ru/NCPCA/LD/CancelLegalServiceApplicationRs/" 
		targetNamespace="http://sbrf.ru/ESB/mq/common">
	<import namespace="http://sbrf.ru/ESB/mq/headers" schemaLocation="./../../Common/Header.xsd"/>
	<import namespace="http://sbrf.ru/NCPCA/LD/CancelLegalServiceApplicationRq/" schemaLocation="./CancelLegalServiceApplicationRq.xsd"/>
	<import namespace="http://sbrf.ru/NCPCA/LD/CancelLegalServiceApplicationRs/" schemaLocation="./CancelLegalServiceApplicationRs.xsd"/>

	<element name="Body">
		<annotation>
			<documentation>Тело сообщения</documentation>
		</annotation>
		<complexType>
			<choice>
				<element ref="canclsarq:CancelLegalServiceApplicationRq"/>
				<element ref="canclsars:CancelLegalServiceApplicationRs"/>			
			</choice>
		</complexType>
	</element>
	<element name="Envelope">
		<annotation>
			<documentation>Сообщение</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element ref="hdr:Header"/>
				<element ref="tns:Body"/>
			</sequence>
		</complexType>
	</element>
</schema>

