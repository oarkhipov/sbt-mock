<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
		xmlns:tns="http://sbrf.ru/ESB/mq/common"
		xmlns:hdr="http://sbrf.ru/ESB/mq/headers" 
		xmlns:glsrirq="http://sbrf.ru/NCPCA/LD/GetLegalServicesReferenceItemRq/" 
		xmlns:glsrirs="http://sbrf.ru/NCPCA/LD/GetLegalServicesReferenceItemRs/" 
		targetNamespace="http://sbrf.ru/ESB/mq/common">
	<import namespace="http://sbrf.ru/ESB/mq/headers" schemaLocation="./../../Common/Header.xsd"/>
	<import namespace="http://sbrf.ru/NCPCA/LD/GetLegalServicesReferenceItemRq/" schemaLocation="./GetLegalServicesReferenceItemRq.xsd"/>
	<import namespace="http://sbrf.ru/NCPCA/LD/GetLegalServicesReferenceItemRs/" schemaLocation="./GetLegalServicesReferenceItemRs.xsd"/>
	
	<element name="Body">
		<annotation>
			<documentation>Тело сообщения</documentation>
		</annotation>
		<complexType>
			<choice>
				<element ref="glsrirq:GetLegalServicesReferenceItemRq"/>
				<element ref="glsrirs:GetLegalServicesReferenceItemRs"/>
			</choice>
		</complexType>
	</element>
	<element name="Envelope">
		<annotation>
			<documentation>Сообщение</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element ref="hdr:Header"/>
				<element ref="tns:Body"/>
			</sequence>
		</complexType>
	</element>
</schema>

