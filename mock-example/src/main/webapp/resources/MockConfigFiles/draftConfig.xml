<MockConfig xmlns="http://sbrf.ru/mockService">
    <!--OPTIONAL-->
    <namespace-aliases>
        <namespace name="soap">http://sbrf.ru/NCP/esb/envelope/</namespace>
    </namespace-aliases>
    <routers>
        <router>
            <!--OPTIONAL-->
            <name></name>
            <!--OPTIONAL (inherited from root)-->
            <namespace-aliases>
                <namespace></namespace>
            </namespace-aliases>
            <expression>/soap:Envelope/soap:Header/soap:operationName</expression>
            <systems>
                <system>CRM</system>
                <system>AMRLIRT</system>
            </systems>
        </router>
    </routers>
    <systems>
        <system name="CRM">
            <pathToXSD>/res/xsd</pathToXSD>
            <rootXsd>CRM.xsd</rootXsd>
            <headerNamespace>http://sbrf.ru/prpc/mq/headers</headerNamespace>
            <!--OPTIONAL (inherited from root)-->
            <namespace-aliases>
                <namespace name="ns1">http://ns1.ru/</namespace>
            </namespace-aliases>
            <integrationPoints>
                <integrationPoint name="Create Task">
                    <!--OPTIONAL-->
                    <operationName>createTaskRs</operationName>
                    <!--OPTIONAL-->
                    <rsRootElementName>createTaskRs</rsRootElementName>
                    <rqRootElementName>createTaskRq</rqRootElementName>
                    <type>Driver/Mock</type>
                    <namespace-aliases>
                        <namespace name="ns2">http://ns2.ru/</namespace>
                    </namespace-aliases>
                    <linkedTagSequence>
                        <tag>ns1:CRM</tag>
                        <tag>ns1:createTaskRq</tag>
                        <tag ns="http://ns2.ru/">comment</tag>
                    </linkedTagSequence>
                    <!--OPTIONAL-->
                    <mappedTagSequence>
                        <mappedTag>
                            <requestMapping>
                                <tag>ns1:CRM</tag>
                                <tag>ns1:createTaskRq</tag>
                                <tag>ns3:id</tag>
                            </requestMapping>
                            <responceMapping>
                                <tag>ns1:CRM</tag>
                                <tag>ns1:createTaskRq</tag>
                                <tag>ns3:id</tag>
                            </responceMapping>
                        </mappedTag>
                        <mappedTag>
                            <requestQuerryValue>/*[name='taxId'][3]</requestQuerryValue>
                            <responceMapping>
                                <tag>ns1:CRM</tag>
                                <tag>ns1:createTaskRq</tag>
                                <tag>ns3:id</tag>
                            </responceMapping>
                        </mappedTag>
                    </mappedTagSequence>
                    <protocol>MQ/SOAP/HTTP</protocol>
                    <!--OPTIONAL-->
                    <incomeQueue>IN</incomeQueue>
                    <!--OPTIONAL-->
                    <outcomeQueue>OUT</outcomeQueue>
                    <xsdFile>create.xsd</xsdFile>
                    <!--OPTIONAL-->
                    <RqXsdFile>created.xsd</RqXsdFile>
                    <!--OPTIONAL-->
                    <dependencies>
                        <dependency>
                            <xsdFile>CommonTypes.xsd</xsdFile>
                        </dependency>
                        <dependency>
                            <xsdFile>CRM.xsd</xsdFile>
                        </dependency>
                    </dependencies>
                    <!--OPTIONAL-->
                    <dataXml>
                        <responce name="default">
                            <anyData>anyData</anyData>
                        </responce>
                    </dataXml>
                </integrationPoint>
            </integrationPoints>
            <!--OPTIONAL-->
            <dependencies>
                <dependency>
                    <xsdFile>CommonTypes.xsd</xsdFile>
                </dependency>
                <dependency>
                    <xsdFile>CRM.xsd</xsdFile>
                </dependency>
            </dependencies>
        </system>
    </systems>
</MockConfig>
