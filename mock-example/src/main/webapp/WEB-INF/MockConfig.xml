                        <tag ns="http://sbrf.ru/legal/enquiry/integration">CreateLegalSalaryAppRq</tag>
                    </xpathValidation>
                    <!-- Тип точки интеграции Заглушка/Драйвер-->
                    <type>Driver</type>
                    <!-- Где лежит корневой элемент-->
                    <!-- Optional -->
                    <xsdFile>SrvCreateLegalSalaryApp.xsd</xsdFile>
                    <!-- Указатель на корневой элемент для генерации тестового сообщения-->
                    <!-- Optional -->
                    <rootElement ns="http://sbrf.ru/ASP/ESB/envelope/">envelope</rootElement>
                </integrationPoint>
