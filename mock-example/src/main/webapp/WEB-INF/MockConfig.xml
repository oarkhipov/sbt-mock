<MockConfig xmlns="http://sbrf.ru/mockService2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://sbrf.ru/mockService2">
    <systems>
        <system name="CRM">
            <!-- XSD с элементами по умолчанию (путь указывается от корневой директории системы WEB-INF/data/CRM/) -->
            <rootXsd>kkmb-crm-header-req.xsd</rootXsd>
            <!-- Селектор для маршрутизации сообщений. Определения, какая точки интеграции должна быть вызвана-->
            <integrationPointSelector>
                <tag ns="http://sbrf.ru/prpc/kkmb/crm/Header/req/10">Message</tag>
                <tag ns="http://sbrf.ru/prpc/kkmb/crm/Header/req/10">OperationName</tag>
            </integrationPointSelector>
            <!-- Протокол интеграции MQ/SOAP. Если используется сразу оба, то необходимо разделить на 2 системы.
             System-MQ, System-SOAP -->
            <protocol>MQ</protocol>

            <queueConnectionFactory>jms/KKMB.DEV.CF</queueConnectionFactory>
            <!-- Входящая очередь заглушки -->
            <mockIncomeQueue>jms/Q.KKMB.TO.CRMORG</mockIncomeQueue>
            <!-- Исходящая очередь заглушки -->
            <mockOutcomeQueue>jms/Q.KKMB.FROM.CRMORG.SYNCRESP</mockOutcomeQueue>
            <!-- Исходящая очередь драйвера -->
            <driverOutcomeQueue>jms/Q.KKMB.FROM.CRMORG</driverOutcomeQueue>
            <!-- Входящая очередь драйвера -->
            <driverIncomeQueue>jms/Q.KKMB.TO.CRMORG.SYNCRESP</driverIncomeQueue>

            <integrationPoints>

                <!--1. driver ContractInfo -->
                <integrationPoint name="ExportContractInfo">
                    <!-- Валидация сообщения, которое должно находиться в точке интеграции -->
                    <xpathValidation>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/Header/req/10">Message</tag>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/ExportContractInfo/req/10">ContractInfo</tag>

                    </xpathValidation>
                    <!-- Тип точки интеграции Заглушка/Драйвер-->
                    <type>Driver</type>
                    <!-- Где лежит корневой элемент-->
                    <!-- Optional -->
                    <xsdFile>kkmb-crm-header-req.xsd</xsdFile>
                    <!-- Указатель на корневой элемент для генерации тестового сообщения-->
                    <!-- Optional -->
                    <rootElement ns="http://sbrf.ru/prpc/kkmb/crm/Header/req/10">Message</rootElement>
                </integrationPoint>

                <!--2. mock ContractID -->
                <integrationPoint name="GetParticipantInfo">
                    <!-- Валидация сообщения, которое должно находиться в точке интеграции -->
                    <!-- Ответ - что угодно, а точнее не требуется -->
                    <xpathValidation>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/Header/resp/10">Message</tag>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/GetParticipant/resp/10">ParticipantInfo</tag>
                    </xpathValidation>
                    <!-- Тип точки интеграции Заглушка/Драйвер-->
                    <type>Mock</type>

                    <!-- Где лежит корневой элемент-->
                    <!-- Optional -->
                    <xsdFile>kkmb-crm-header-resp.xsd</xsdFile>
                    <!-- Указатель на корневой элемент для генерации тестового сообщения-->
                    <!-- Optional -->
                    <rootElement ns="http://sbrf.ru/prpc/kkmb/crm/Header/resp/10">Message</rootElement>
                    <!-- Требуется ли ответ -->
                    <answerRequired>true</answerRequired>
                </integrationPoint>

                <!--3. driver ContractClose-->
                <integrationPoint name="CloseContractInfo">
                    <!-- Валидация сообщения, которое должно находиться в точке интеграции -->
                    <xpathValidation>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/Header/req/10">Message</tag>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/ContractClose/req/10">ContractClose</tag>
                    </xpathValidation>
                    <!-- Тип точки интеграции Заглушка/Драйвер-->
                    <type>Driver</type>
                    <!-- Где лежит корневой элемент-->
                    <!-- Optional -->
                    <xsdFile>kkmb-crm-header-req.xsd</xsdFile>
                    <!-- Указатель на корневой элемент для генерации тестового сообщения-->
                    <!-- Optional -->
                    <rootElement ns="http://sbrf.ru/prpc/kkmb/crm/Header/req/10">Message</rootElement>
                </integrationPoint>

                <!--4. mock UpdateContractInfo -->
                <integrationPoint name="UpdateContractInfo">
                    <!-- Валидация сообщения, которое должно находиться в точке интеграции -->
                    <!-- Ответ - что угодно, а точнее не требуется -->
                    <xpathValidation>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/Header/resp/10">Message</tag>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/UpdateContractInfo/resp/10">UpdateContractInfo</tag>
                    </xpathValidation>
                    <!-- Тип точки интеграции Заглушка/Драйвер-->
                    <type>Mock</type>

                    <!-- Где лежит корневой элемент-->
                    <!-- Optional -->
                    <xsdFile>kkmb-crm-header-resp.xsd</xsdFile>
                    <!-- Указатель на корневой элемент для генерации тестового сообщения-->
                    <!-- Optional -->
                    <rootElement ns="http://sbrf.ru/prpc/kkmb/crm/Header/resp/10">Message</rootElement>
                    <!-- Требуется ли ответ -->
                    <answerRequired>true</answerRequired>
                </integrationPoint>

                <!--5. Driver ReferenceData -->
                <integrationPoint name="UpdateReferenceData">
                    <!-- Валидация сообщения, которое должно находиться в точке интеграции -->
                    <!-- Ответ - что угодно, а точнее не требуется -->
                    <xpathValidation>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/Header/req/10">Message</tag>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/ReferenceData/req/10">ReferenceData</tag>
                    </xpathValidation>
                    <!-- Тип точки интеграции Заглушка/Драйвер-->
                    <type>Driver</type>

                    <!-- Где лежит корневой элемент-->
                    <!-- Optional -->
                    <xsdFile>kkmb-crm-header-req.xsd</xsdFile>
                    <!-- Указатель на корневой элемент для генерации тестового сообщения-->
                    <!-- Optional -->
                    <rootElement ns="http://sbrf.ru/prpc/kkmb/crm/Header/req/10">Message</rootElement>

                </integrationPoint>

                <!--6. driver ProblemGroup -->
                <integrationPoint name="ProblemGroup">
                    <!-- Валидация сообщения, которое должно находиться в точке интеграции -->
                    <!-- Ответ - что угодно, а точнее не требуется -->
                    <xpathValidation>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/Header/req/10">Message</tag>
                        <tag ns="http://sbrf.ru/prpc/kkmb/crm/ProblemGroup/req/10">ProblemGroup</tag>
                    </xpathValidation>
                    <!-- Тип точки интеграции Заглушка/Драйвер-->
                    <type>Driver</type>
                    <!-- Где лежит корневой элемент-->
                    <!-- Optional -->
                    <xsdFile>kkmb-crm-header-req.xsd</xsdFile>
                    <!-- Указатель на корневой элемент для генерации тестового сообщения-->
                    <!-- Optional -->
                    <rootElement ns="http://sbrf.ru/prpc/kkmb/crm/Header/req/10">Message</rootElement>

                </integrationPoint>
            </integrationPoints>
        </system>
