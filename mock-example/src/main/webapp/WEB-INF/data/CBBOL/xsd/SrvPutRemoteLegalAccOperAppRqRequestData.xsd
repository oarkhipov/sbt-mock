   </xsd:complexType>
   <xsd:complexType name="Registration">
      <xsd:annotation>
         <xsd:documentation>Регистрация</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="OGRN" minOccurs="0" type="st:StateRegPrimeNum_Type">
            <xsd:annotation>
               <xsd:documentation>ОГРН</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OGRNAssignDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата присвоения ОГРН</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OGRNIssueDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата выдачи ОГРН</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ShortName">
            <xsd:annotation>
               <xsd:documentation>Краткое наименование</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="160"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="FullName">
            <xsd:annotation>
               <xsd:documentation>Полное наименование</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="300"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="ShortNameInEnglish" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Английское написание краткого имени</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="80"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="FullNameInEnglish" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Английское написание полного имени</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegistrationSertificateNumber" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Серия и номер свидетельства</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="11"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="RegistrationSertificateIssueDate"
                      minOccurs="0"
                      type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата выдачи свидетельства о регистрации</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegistrationAuthority" minOccurs="0" type="dt:DefPosInteger">
            <xsd:annotation>
               <xsd:documentation>Регистрирующий орган
                            Справочник «Регистрирующие органы»
                        </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OldShortName">
            <xsd:annotation>
               <xsd:documentation>Старое краткое наименование</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OldName">
            <xsd:annotation>
               <xsd:documentation>Старое наименование</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OldShortNameInEnglish">
            <xsd:annotation>
               <xsd:documentation>Старое краткое наименование (англ.)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OldNameInEnglish">
            <xsd:annotation>
               <xsd:documentation>Старое полное наименование (англ.)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TermFrom">
            <xsd:annotation>
               <xsd:documentation>Срок действия старого поля с</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TermTill">
            <xsd:annotation>
               <xsd:documentation>Срок действия старого поля по</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DeclaredFound">
            <xsd:annotation>
               <xsd:documentation>Объявленный уставной фонд</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PayedFound">
            <xsd:annotation>
               <xsd:documentation>Оплаченный уставной фонд</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Control">
            <xsd:annotation>
               <xsd:documentation>Органы управления</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Amount">
            <xsd:annotation>
               <xsd:documentation>Численность</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="YearROI">
            <xsd:annotation>
               <xsd:documentation>Годовая выручка</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="From">
            <xsd:annotation>
               <xsd:documentation>С</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Till">
            <xsd:annotation>
               <xsd:documentation>По</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="License">
            <xsd:annotation>
               <xsd:documentation>Лицензия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SpecialData">
      <xsd:annotation>
         <xsd:documentation>Спец. данные</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ClientType">
            <xsd:annotation>
               <xsd:documentation>Тип клиента
                            1 – юридическое лицо
                            2 – индивидуальный предприниматель
                            3 - адвокат
                            4 - нотариус
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
                  <xsd:enumeration value="3"/>
                  <xsd:enumeration value="4"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="Resident" minOccurs="0" type="dt:Boolean">
            <xsd:annotation>
               <xsd:documentation>Признак резидента
                            Принимает значения:
                            1 – резидент
                            0 – нерезидент
                        </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Risk" minOccurs="0" type="dt:Boolean">
            <xsd:annotation>
               <xsd:documentation>Признак наличия риска
                            Принимает значения:
                            1 – есть риск
                            0 – нет риска
                        </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RiskExplanation" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Обоснование риска</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CountryOfRegistration" minOccurs="0" type="st:SSM_Type">
            <xsd:annotation>
               <xsd:documentation>Страна регистрации</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CountryOfAccreditation" minOccurs="0" type="st:SSM_Type">
            <xsd:annotation>
               <xsd:documentation>Страна аккредитации</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RepresentedInCountry" minOccurs="0" type="st:SSM_Type">
            <xsd:annotation>
               <xsd:documentation>Представительство</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AuthorizedCapital" minOccurs="0" type="dt:PosFloat">
            <xsd:annotation>
               <xsd:documentation>Уставной капитал</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="Identifiers">
      <xsd:annotation>
         <xsd:documentation>Идентификаторы</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ClientId" type="dt:PosInteger">
            <xsd:annotation>
               <xsd:documentation>ID клиента в РКО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="State">
            <xsd:annotation>
               <xsd:documentation>Состояние
                            Активный – 1
                            Неактивный – 2
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="CreateDate" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата создания</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CloseDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата закрытия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ClientAddress">
      <xsd:annotation>
         <xsd:documentation>Адрес клиента</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="AddressType">
            <xsd:annotation>
               <xsd:documentation>Принимает значения:
1- Прописка; 
2 – Место жительства; 
5- Почтовый; 
6- Юридический; 
7- Фактический;
8 – ЕГРЮЛ;
9 - Почтовый адрес представительства.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
                  <xsd:enumeration value="5"/>
                  <xsd:enumeration value="6"/>
                  <xsd:enumeration value="7"/>
                  <xsd:enumeration value="8"/>
                  <xsd:enumeration value="9"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="UnnormalizedAddress" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Ненормализованный адрес</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Country" minOccurs="0" type="st:SSM_Type">
            <xsd:annotation>
               <xsd:documentation>Страна</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PostalCode" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Почтовый индекс</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="50"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="IdentificationCode" minOccurs="0" type="st:KLADR_Type">
            <xsd:annotation>
               <xsd:documentation>Идентификационный код объекта</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OKATO" minOccurs="0" type="st:OKATO_Type">
            <xsd:annotation>
               <xsd:documentation>Код ОКАТО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RegionType" minOccurs="0" type="st:KLADR_Type">
            <xsd:annotation>
               <xsd:documentation>Тип региона</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Region" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Регион</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="60"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="AreaType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Тип района</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="10"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="Area" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Район</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="60"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="CityType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Тип города</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="10"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="City" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Город</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="100"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="PlaceType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Тип населенного пункта</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="10"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="Place" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Населенный пункт</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="40"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="StreetType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Тип улицы</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="10"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="Street" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Улица</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="50"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="House" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Дом (владение)</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="20"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="Corpus" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Корпус</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="20"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="Building" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Строение</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="20"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="FlatorOffice" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Квартира/офис</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="20"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ClientContact">
      <xsd:annotation>
         <xsd:documentation>Контакт клиента</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ContactCategory">
            <xsd:annotation>
               <xsd:documentation>Категория контакта
                            1 - Телефон
                            2 - Факс
                            3 - Телеграф
                            4 - Телефакс
                            5 - Телетайп
                            6 - Телекс
                            7 - Адрес в сети Интернет
                            8 - E-mail
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
                  <xsd:enumeration value="3"/>
                  <xsd:enumeration value="4"/>
                  <xsd:enumeration value="5"/>
                  <xsd:enumeration value="6"/>
                  <xsd:enumeration value="7"/>
                  <xsd:enumeration value="8"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="ContactType" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Тип контакта
                            1 – Телефон N 1
                            2 – Телефон N 2
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="ContactName" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Наименование контакта</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ContactData" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Значение контактной информации</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
