            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="TaxId" minOccurs="0" type="st:TaxId_Type">
            <xsd:annotation>
               <xsd:documentation>ИНН</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TaxRegReasonCode"
                      minOccurs="0"
                      type="st:TaxRegReasonCode_Type">
            <xsd:annotation>
               <xsd:documentation>КПП</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="KIO" minOccurs="0" type="st:KIO_Type">
            <xsd:annotation>
               <xsd:documentation>КИО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TaxInspectorate" minOccurs="0" type="dt:DefPosInteger">
            <xsd:annotation>
               <xsd:documentation>Налоговая инспекция
