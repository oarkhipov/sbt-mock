            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OGRN" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>ОГРН</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OGRNRegistrationDate" minOccurs="0" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>дата регистрации</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OGRNSeries" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>серия свид-ва</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OGRNNumber" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>№ свид-ва</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OGRNAuthority" minOccurs="0" type="xsd:string">
