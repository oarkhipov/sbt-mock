               <xsd:documentation>регистрирующий орган</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientAddress" minOccurs="0" type="tns:ClientAddress"/>
         <xsd:element name="LegalCapitalPercent" minOccurs="0" type="xsd:decimal">
