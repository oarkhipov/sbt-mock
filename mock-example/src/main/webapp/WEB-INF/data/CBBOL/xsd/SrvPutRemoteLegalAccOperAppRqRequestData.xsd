      <xsd:annotation>
         <xsd:documentation>Договор банковского счета юридического лица</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="BankSubdivision" type="tns:BankSubdivision">
            <xsd:annotation>
               <xsd:documentation>Подразделение
                            Справочник подразделений Банка
                        </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientId" type="dt:PosInteger">
            <xsd:annotation>
               <xsd:documentation>ID клиента в РКО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AgreementIdentifier" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Идентификатор договора</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AgreementNumber" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Номер договора</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AgreementType" type="dt:DefPosInteger">
            <xsd:annotation>
               <xsd:documentation>Тип договора
                            Справочник типов дого-воров РКО: «Договор РКО рубли», «Договор РКО в валюте», и пр.
                        </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AgreementStatus">
            <xsd:annotation>
               <xsd:documentation>Статус договора
                            1 – действует
                            2 – закрыт
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="AgreementIssueDate" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата заключения договора</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AgreementCancelDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата расторжения договора</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Operator" type="dt:PosInteger">
            <xsd:annotation>
               <xsd:documentation>Операционист</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MainAccountNumber" type="st:AccountNumber_Type">
            <xsd:annotation>
               <xsd:documentation>Номер основного счета</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AccountBalance" type="dt:PosFloat">
            <xsd:annotation>
               <xsd:documentation>Остаток по счету</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ArrestByAccountPresence" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Наличие ареста по счету (для договора РКО)
                            1 – По дебету
                            2 – Полный
                            3 – Нет
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
                  <xsd:enumeration value="3"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="RKOAgreementDetails"
                      minOccurs="0"
                      type="tns:RKOAgreementDetails">
            <xsd:annotation>
               <xsd:documentation>Параметры договора РКО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RKOAgreementDetails">
      <xsd:annotation>
         <xsd:documentation>Параметры договора РКО</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TariffPlan"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:TariffPlan"/>
         <xsd:element name="DetermineTariffs" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Определить тарифы</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TariffGroup" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Группа тарифов</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientAccount"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:ClientAccount"/>
         <xsd:element name="PercentCalculationAlgorithm" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Алгоритм начисления %
                            Справочник
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:PosInteger">
                  <xsd:maxLength value="10"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="DeterminePercent" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Опредилить %</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PercentRateGroup" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Группа % ставок</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DateBeginPercentRate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата начала действия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DateEndPercentRate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата окончания действия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PartialRatedPeriodCalculation"
                      minOccurs="0"
                      type="dt:Boolean">
            <xsd:annotation>
               <xsd:documentation>Расчет за неполный расчетный период при открытии счета</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CurrentRatedPeriodCalculation" type="dt:Boolean">
            <xsd:annotation>
               <xsd:documentation>Расчет, если счет закрывается в текущем периоде</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OpeningReason" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Основание открытия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AgreementUserType" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Пользовательский тип договора
                            Каждый символ контролируется по справочнику
                        </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SignPatternCard">
      <xsd:annotation>
         <xsd:documentation>Карточка с образцами подписей</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ClientId" minOccurs="0" type="dt:PosInteger">
            <xsd:annotation>
               <xsd:documentation>ID клиента в РКО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BankSubdivision" minOccurs="0" type="tns:BankSubdivision">
            <xsd:annotation>
               <xsd:documentation>Подразделение банка</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AgreementNumber" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Номер договора</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AccountNumber" minOccurs="0" type="st:AccountNumber_Type">
            <xsd:annotation>
               <xsd:documentation>Номер счета</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CardType" nillable="true" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Тип карточки
                            1 – Постоянная
                            2 – Временная
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="RegistrationDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата регистрации</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ActivationDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата активации</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CloseDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата закрытия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CardRegistartionMethod" nillable="true" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Способ оформления карточки
                            1 – Нотариально
                            2 – Банком
                            Зарезервировано. В данной версии не используется.
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="Sign" maxOccurs="unbounded" type="tns:Sign">
            <xsd:annotation>
               <xsd:documentation>Подпись</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="Sign">
      <xsd:annotation>
         <xsd:documentation>Подпись</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RKONaturalPersonCode" type="dt:DefInteger">
            <xsd:annotation>
               <xsd:documentation>Код физического лица в РКО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SeqNumber" minOccurs="0" type="dt:DefInteger">
            <xsd:annotation>
               <xsd:documentation>Последовательный номер физ.лица</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SignType">
            <xsd:annotation>
               <xsd:documentation>Тип подписи
                            1 – первая подпись
                            2 – вторая подпись
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="Position" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Должность</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AuthorityStartDate" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Дата начала полномочий</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AuthorityEndDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата окончания полномочий</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ClientAccount">
      <xsd:annotation>
         <xsd:documentation>Счет клиента для удержания комиссии</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="AccountNumber" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Номер счета</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExternalAttribute" minOccurs="0" type="dt:Boolean">
            <xsd:annotation>
               <xsd:documentation>Признак внешнего счета.

Принимает значения:
1 – внешний;
0 – внутренний.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RecipientName" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>Наименование получателя</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TaxID" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>ИНН</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TaxRegReasonCode" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>КПП</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BIK" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>БИК банка получателя</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SWIFTCode" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>SWIFT код банка посредника</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClearingCode" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>Код клиринга банка посредника</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BankBeneficiarySWIFTCode" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>SWIFT код банка бенефициара</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DCode" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>57D банка бенефициара</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BeneficiaryIBANCode" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>IBAN клиента бенефициара</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientBeneficiarySWIFTCode" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>SWIFT код клиента бенефициара</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientBeneficiaryName" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>Наименование клиента бенефициара</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientBeneficiaryTaxID" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>ИНН клиента бенефициара</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientBeneficaryAddress" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>Адрес клиента бенефициара</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientBeneficiaryCountry" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>Город, страна клиента бенефициара</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AccountPurpose" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>Назначение счета (Справочник «Назначение счета»)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="IdentityDocument">
      <xsd:annotation>
         <xsd:documentation>ДУЛ</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DocumentType" type="dt:DefPosInteger">
            <xsd:annotation>
               <xsd:documentation>Тип документа
                                        ЦАС НСИ «pb_document»
                                    </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DocumentStatus">
            <xsd:annotation>
               <xsd:documentation>Статус документа
                                        0 - не задано
                                        1 - активный
                                        2 - неактивный
                                        3 - с истекшей датой действия
                                    </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="0"/>
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
                  <xsd:enumeration value="3"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="DocumentSeries" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Серия ДУЛ</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DocumentNumber" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Номер документа</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата выдачи</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueAuthority" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Кем выдан документ</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueAuthorityCode" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Код подразделения, выдавшего документ</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ExpirationDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Планируемая дата окончания срока действия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="TariffPlan">
      <xsd:annotation>
         <xsd:documentation>Тарифный план</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TariffPlanCode">
            <xsd:annotation>
               <xsd:documentation>Тарифный план</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="255"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="TariffPlanStartDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата начала </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TariffPlanEndDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата окончания</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="BankSubdivision">
      <xsd:annotation>
         <xsd:documentation>Подразделение банка</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="TB" type="st:TB_Type">
            <xsd:annotation>
               <xsd:documentation>Номер ТБ</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OSB" type="st:OSB_Type">
            <xsd:annotation>
               <xsd:documentation>Номер ОСБ</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="VSP" type="st:VSP_Type">
            <xsd:annotation>
               <xsd:documentation>Номер ВСП</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DocumentsInfo">
      <xsd:annotation>
         <xsd:documentation>Информация по каталогу документов</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DocumentLinks"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:DocumentLinks">
            <xsd:annotation>
               <xsd:documentation>Каталоги документов</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DocumentLinks">
      <xsd:annotation>
         <xsd:documentation>Каталоги документов</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="DocumentLink" minOccurs="0" type="dt:String">
            <xsd:annotation>
               <xsd:documentation>Ссылка на документ</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
</xsd:schema>
