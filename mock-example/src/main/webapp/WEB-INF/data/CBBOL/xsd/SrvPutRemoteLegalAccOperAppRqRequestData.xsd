<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://sbrf.ru/prpc/bbmo/10/SrvPutRemoteLegalAccOperAppRq/Data/"
            xmlns:tns="http://sbrf.ru/prpc/bbmo/10/SrvPutRemoteLegalAccOperAppRq/Data/"
            xmlns:st="http://sbrf.ru/prpc/bbmo/simpleTypes/10"
            xmlns:ct="http://sbrf.ru/prpc/bbmo/commonTypes/10"
            xmlns:dt="http://sbrf.ru/prpc/bbmo/dataTypes/10"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://sbrf.ru/prpc/bbmo/10/SrvPutRemoteLegalAccOperAppRq/Data/"
            elementFormDefault="qualified">
   <xsd:import namespace="http://sbrf.ru/prpc/bbmo/dataTypes/10"
               schemaLocation="../../../xsd/CBBOL/commonTypes/dataTypes.xsd"/>
   <xsd:import namespace="http://sbrf.ru/prpc/bbmo/simpleTypes/10"
               schemaLocation="../../../xsd/CBBOL/commonTypes/simpleTypes.xsd"/>
   <xsd:import namespace="http://sbrf.ru/prpc/bbmo/commonTypes/10"
               schemaLocation="../../../xsd/CBBOL/commonTypes/commonTypes.xsd"/>
   <xsd:complexType name="Header">
      <xsd:annotation>
         <xsd:documentation>Заголовок сообщения</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence xmlns="">
         <xsd:element name="KD4SoapHeaderV2" type="dt:String" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Заголовок SOAP-сообщения</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AsyncHeader">
            <xsd:annotation>
               <xsd:documentation>Заголовок SOAP-сообщения</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="message-id" type="st:UUID" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Уникальный идентификатор сообщения, передаваемого в рамках SOAP-пакета</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="request-time" type="dt:DateTime" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Системное время при отправке сообщения АС-отправителем с учетом временной зоны</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="correlation-id" type="st:UUID" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Идентификатор корреляции</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="eis-name" type="st:SystemName_Type" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Идентификатор системы инициатора запроса/ответа</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="system-id"
                               type="st:SystemName_Type"
                               minOccurs="0"
                               maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>Идентификатор системы получателя запроса</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="operation-name" type="st:OperationName_Type" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Название операции вызываемого сервиса</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="operation-version" type="dt:DefString" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Версия операции</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="user-id" type="dt:DefString" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Идентификатор пользователя в системе инициаторе запроса/ответа</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="user-name" type="dt:DefString" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Полное ФИО пользователя в системе инициаторе запроса/ответа</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="proc-inst-tb" type="st:TB_Type" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Код ТБ</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:any namespace="##other"
                           processContents="lax"
                           minOccurs="0"
                           maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>Точка расширения заголовка (не используется, добавлено на персективу)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:any>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="data" type="dataValue"/>
   <xsd:complexType name="dataValue">
      <xsd:sequence>
         <xsd:element name="request"
                      type="tns:SrvPutRemoteLegalAccOperAppRq"
                      maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Ответ на запрос</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SrvPutRemoteLegalAccOperAppRq">
      <xsd:complexContent>
         <xsd:extension base="ct:DataProcessingRequest">
            <xsd:sequence>
               <xsd:element name="SoapHeader" type="tns:Header" minOccurs="0"/>
               <xsd:element name="RemoteClient" minOccurs="0" type="dt:Boolean">
                  <xsd:annotation>
                     <xsd:documentation>Признак удалённого создания клиента</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="RemoteSystemId" minOccurs="0" type="dt:String">
                  <xsd:annotation>
                     <xsd:documentation>Система источник запрос в ЕКС</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IsAdditionalAccount" minOccurs="0" type="dt:Boolean">
                  <xsd:annotation>
                     <xsd:documentation>Признак дополнительного или основного счета</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="IsBankCardProcessing" minOccurs="0" type="dt:Boolean">
                  <xsd:annotation>
                     <xsd:documentation>Признак формирования банковской карточки клиента</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ClientReferenceData" type="tns:ClientReferenceData">
                  <xsd:annotation>
                     <xsd:documentation>Справочные данные клиента</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="LegalPersonApplication" type="tns:LegalPersonApplication">
                  <xsd:annotation>
                     <xsd:documentation>Заявление юридического лица</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="DocumentsInfo" type="tns:DocumentsInfo">
                  <xsd:annotation>
                     <xsd:documentation>Информация по каталогу документов</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="name"/>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="LegalPersonApplication">
      <xsd:annotation>
         <xsd:documentation>Заявление юридического лица</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ApplicationId" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>ID заявления (не используется, добавлено на перспективу)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OperationType" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Тип операции (не используется, добавлено на перспективу)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientId" minOccurs="0" type="dt:PosInteger">
            <xsd:annotation>
               <xsd:documentation>ID клиента</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TaxId" type="st:TaxId_Type">
            <xsd:annotation>
               <xsd:documentation>ИНН Клиента</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CustomerFullName" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Полное наименование клиента</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BankSubdivision" type="tns:BankSubdivision">
            <xsd:annotation>
               <xsd:documentation>Подразделение
Справочник подразделений Банка</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="InitDate" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата инициации заявления</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AgreementIdentifier" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Идентификатор договора</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AgreementNumber" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Номер договора</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MainAccountNumber" minOccurs="0" type="st:AccountNumber_Type">
            <xsd:annotation>
               <xsd:documentation>Номер основного счета</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TaxRegReasonCode"
                      minOccurs="0"
                      type="st:TaxRegReasonCode_Type">
            <xsd:annotation>
               <xsd:documentation>КПП клиента</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BarCode" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Штрих-код</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="NaturalPersonWithAuthority">
      <xsd:annotation>
         <xsd:documentation>Физическое, должностное лицо и учредитель</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="RKOCode" minOccurs="0" type="dt:DefInteger">
            <xsd:annotation>
               <xsd:documentation>Код физического лица в РКО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SeqNumber" minOccurs="0" type="dt:DefInteger">
            <xsd:annotation>
               <xsd:documentation>Последовательный номер физ.лица</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RelationToClient">
            <xsd:annotation>
               <xsd:documentation>Отношение к клиенту
Принимает значения:
1 – Руководитель; 
2 – Гл.Бухгалтер; 
3 – ИП;
4 – выгодоприобретатель;
5 – прочее.
</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LastName">
            <xsd:annotation>
               <xsd:documentation>Фамилия</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="160"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="FirstName">
            <xsd:annotation>
               <xsd:documentation>Имя</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="160"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="MiddleName" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Отчество</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="160"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="LastNameInGenitiveCase" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Фамилия в родительном падеже</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="160"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="FirstNameInGenitiveCase" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Имя в родительном падеже</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="160"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="MiddleNameInGenitiveCase" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Отчество в родительном падеже</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="160"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="LastNameInEnglish" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Фамилия на английском</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="160"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="FirstNameInEnglish" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Имя на английском</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="160"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="MiddleNameInEnglish" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Отчество на английском</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:String">
                  <xsd:maxLength value="160"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="Sex" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Пол
                            1 - мужской
                            2 - женский
                            3 - не указан
                        </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
               <xsd:restriction base="dt:DefPosInteger">
                  <xsd:enumeration value="1"/>
                  <xsd:enumeration value="2"/>
                  <xsd:enumeration value="3"/>
               </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="BirthDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата рождения</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BirthPlace" minOccurs="0" type="dt:DefString">
            <xsd:annotation>
               <xsd:documentation>Место рождения</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Nationality" minOccurs="0" type="st:SSM_Type">
            <xsd:annotation>
               <xsd:documentation>Гражданство</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Resident" minOccurs="0" type="dt:Boolean">
            <xsd:annotation>
               <xsd:documentation>Признак резидента
                            Принимает значения:
                            1 – резидент
                            0 – нерезидент
                        </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TaxResidentChangeDate" minOccurs="0" type="dt:Date">
            <xsd:annotation>
               <xsd:documentation>Дата изменения налоговой резидентности</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TaxId" minOccurs="0" type="st:TaxId_Type">
            <xsd:annotation>
               <xsd:documentation>ИНН</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SocialStatusType" minOccurs="0" type="xsd:int">
            <xsd:annotation>
               <xsd:documentation>Вид социального статуса</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IdentityDocument"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:IdentityDocument"/>
         <xsd:element name="ClientAddress"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:ClientAddress">
            <xsd:annotation>
               <xsd:documentation>Адреса клиента</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientContact"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:ClientContact">
            <xsd:annotation>
               <xsd:documentation>Контакты клиента</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AuthorityBase" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Основание полномочий</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="DocumentActAuthority" minOccurs="0" type="dt:DefString">
                     <xsd:annotation>
                        <xsd:documentation>Действует на основании</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="DocumentSeriesNumber" minOccurs="0" type="dt:DefString">
                     <xsd:annotation>
                        <xsd:documentation>Серия и номер документа</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="DocumentDate" minOccurs="0" type="dt:Date">
                     <xsd:annotation>
                        <xsd:documentation>Дата документа</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="OfficailInfo" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Информация о должности</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="BasedOnDocument" minOccurs="0" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>Действует на основании документа</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BasedOnDocumentSeries" minOccurs="0" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>Серия документа-основания полномочий</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BasedOnDocumentNumber" minOccurs="0" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>Номер документа-основания полномочий</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TrusteeSign" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Признак доверенного лица</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="OperationType" minOccurs="0" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>Вид операций</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="AccountNumber" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Номер счета</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Comment" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation>Комментарий</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ClientReferenceData">
      <xsd:annotation>
         <xsd:documentation>Справочные данные клиента</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="ClientInformation" minOccurs="0" type="tns:ClientInformation">
            <xsd:annotation>
               <xsd:documentation>Информация о клиенте</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="naturalPerson"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:NaturalPersonWithAuthority">
            <xsd:annotation>
               <xsd:documentation>Физическое лицо</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="LegalPersonBankAccountAgreement"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:LegalPersonBankAccountAgreement">
            <xsd:annotation>
               <xsd:documentation>Договор банковского счета юридического лица</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SignPatternCard"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:SignPatternCard">
            <xsd:annotation>
               <xsd:documentation>Карточка с образцами подписей</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ClientInformation">
      <xsd:annotation>
         <xsd:documentation>Информация о клиенте</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Identifiers" type="tns:Identifiers">
            <xsd:annotation>
               <xsd:documentation>Идентификаторы</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SpecialData" type="tns:SpecialData">
            <xsd:annotation>
               <xsd:documentation>Спец. данные</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MainDetails" type="tns:MainDetails">
            <xsd:annotation>
               <xsd:documentation>Основные реквизиты</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="FounderLegalPerson"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:FounderLegalPerson"/>
         <xsd:element name="BankSubdivision" minOccurs="0" type="tns:BankSubdivision">
            <xsd:annotation>
               <xsd:documentation>Подразделение
                            Справочник подразделений Банка
                        </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ClientAddress"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:ClientAddress"/>
         <xsd:element name="ClientContact"
                      minOccurs="0"
                      maxOccurs="unbounded"
                      type="tns:ClientContact"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="MainDetails">
      <xsd:annotation>
         <xsd:documentation>Основные реквизиты</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Registration" type="tns:Registration">
            <xsd:annotation>
               <xsd:documentation>Регистрация</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TaxInspectorateInfo"
                      minOccurs="0"
                      type="tns:TaxInspectorateInfo">
            <xsd:annotation>
               <xsd:documentation>Налоговая инспекция</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RosstatCodes" minOccurs="0" type="tns:RosstatCodes">
            <xsd:annotation>
               <xsd:documentation>Коды Росстата</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RosstatCodes">
      <xsd:annotation>
         <xsd:documentation>Коды Росстата</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="OKATO" minOccurs="0" type="st:OKATO_Type">
            <xsd:annotation>
               <xsd:documentation>ОКАТО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OKPO" minOccurs="0" type="st:OKPO_Type">
            <xsd:annotation>
               <xsd:documentation>ОКПО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OKFS" minOccurs="0" type="st:OKFS_Type">
            <xsd:annotation>
               <xsd:documentation>ОКФС</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
