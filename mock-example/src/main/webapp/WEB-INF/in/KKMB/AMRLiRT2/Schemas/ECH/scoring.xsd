<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
           xmlns="http://www.sbrf.ru/CreditHistoryScoring"
           targetNamespace="http://www.sbrf.ru/CreditHistoryScoring"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:element name="scoring" type="ScoringType" nillable="false"/>

    <xs:complexType name="ScoringType">
        <xs:annotation>
            <xs:documentation>Корневой элемент</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element type="HeaderType" name="header" minOccurs="1" maxOccurs="1" nillable="false"/>
            <xs:element type="PayloadType" name="payload" minOccurs="1" maxOccurs="1" nillable="false"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ParticipantsType">
        <xs:annotation>
            <xs:documentation>Список участников сделки</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element type="ScoringParticipantType" name="participant" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="HeaderType">
        <xs:annotation>
            <xs:documentation>Заголовок, обязательно должен присутствовать</xs:documentation>
        </xs:annotation>
        <xs:sequence>

            <xs:element name="targetInstance" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Целевой экземпляр для приема</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="100"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="externalRef" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Внешний идентификатор сделки</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="150"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="timestamp" minOccurs="1" maxOccurs="1" type="xs:dateTime" nillable="false">
                <xs:annotation>
                    <xs:documentation>Дата/время создания запроса</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:choice minOccurs="1" maxOccurs="1">
                <xs:element name="processing" type="ScoringProcessingType" nillable="false"/>
                <xs:element name="reprocessing" type="ScoringReprocessingType" nillable="false"/>
            </xs:choice>

        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PayloadType">
        <xs:annotation>
            <xs:documentation>Сообщение (запрос/ответ), должен присутствовать либо запрос либо ответ</xs:documentation>
        </xs:annotation>
        <!--
        <xs:sequence>
            <xs:element type="RequestType" name="request" minOccurs="0" maxOccurs="1"/>
            <xs:element type="ResponseType" name="response" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        -->
        <xs:choice minOccurs="1" maxOccurs="1">
            <xs:element type="RequestType" name="request"/>
            <xs:element type="ResponseType" name="response"/>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="ScoringReprocessingType">
        <xs:annotation>
            <xs:documentation>Запрос на репроцессинг сделки</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="originalDealId" type="xs:long" minOccurs="1" maxOccurs="1" nillable="false">
                <xs:annotation>
                    <xs:documentation>Идентификатор сделки</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ScoringProcessingType">
        <xs:annotation>
            <xs:documentation>Запрос на оценку КИ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element type="xs:string" name="uid" minOccurs="1" maxOccurs="1" nillable="false">
                <xs:annotation>
                    <xs:documentation>Уникальный в рамках системы - источника идентификатор запроса/ответа. Для запроса(request) с UID = ZZZ ответ(response) придет с таким же UID = ZZZ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sourceSystem" type="SourceSystemDictType" minOccurs="1" maxOccurs="1" nillable="false">
                <xs:annotation>
                    <xs:documentation>Система - источник запроса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="scoringType" type="ScoringTypeDictType" minOccurs="1" maxOccurs="1" nillable="false">
                <xs:annotation>
                    <xs:documentation>Тип оценки</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DealScoringResultType">
        <xs:annotation>
            <xs:documentation>Оценка КИ по сделке</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element type="xs:string" name="sid">
                <xs:annotation>
                    <xs:documentation>СИД типа оценки КИ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="name">
                <xs:annotation>
                    <xs:documentation>Наименование типа оценки КИ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="value">
                <xs:annotation>
                    <xs:documentation>Результат оценки по типу оценки КИ</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BlockDataASSDType">
        <xs:sequence>
            <xs:element type="xs:decimal" name="_dcas_ResidualDebtAndArrear" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>АС СД: общий остаток ссудной задолжности (сумма срочной и просроченной задолженности по основному долгу)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="productType">
                <xs:annotation>
                    <xs:documentation>Код типа продукта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="productCode">
                <xs:annotation>
                    <xs:documentation>Код продукта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="subProductCode">
                <xs:annotation>
                    <xs:documentation>Код субпродукта</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="LiabilityArrearsBlockType">
        <xs:sequence>
            <xs:element type="xs:string" name="yearNumber">
                <xs:annotation>
                    <xs:documentation>Номер анализируемого года</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:date" name="DtYearBegin">
                <xs:annotation>
                    <xs:documentation>Дата начала годового интервала</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:date" name="DtYearEnd">
                <xs:annotation>
                    <xs:documentation>Дата окончания периода годового интеравала</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="arrearsCount_1_5" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Количество случаев возникновения просроченной задолжности в период 1-5</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="arrearsCount_6_30" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Количество случаев возникновения просроченной задолжности в период 6-30</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="arrearsCount_1_30" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Количество случаев возникновения просроченной задолжности в период 1-30</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="arrearsCount_31_60" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Количество случаев возникновения просроченной задолжности в перио 31-60</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="arrearsCount_61_90" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Количество случаев возникновения просроченной задолжности в период 61-90</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="arrearsCount_91_120" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Количество случаев возникновения просроченной задолжности в период 91-120</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="arrearsCount_121_150" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Количество случаев возникновения просроченной задолжности в период 121-150</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="arrearsCount_151_180" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Количество случаев возникновения просроченной задолжности в период 151-180</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="arrearsCount_180" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Количество случаев возникновения просроченной задолжности в период свыше 180</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:int" name="nMaxDelayPayments">
                <xs:annotation>
                    <xs:documentation>Количество случаев возникновения просроченной задолженности в годовом интервале</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:int" name="arrearsCount_PerYear">
                <xs:annotation>
                    <xs:documentation>Максимальная продолжительность задержки платежей (дней) в анализируемом интервале</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ParticipantCreditHistoriesType">
        <xs:annotation>
            <xs:documentation>Список КИ участника сделки</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element type="CreditHistoryType" name="creditHistory" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CreditHistoryType">
        <xs:annotation>
            <xs:documentation>КИ участника сделки</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="sourceCB" type="CreditBureauDictType">
                <xs:annotation>
                    <xs:documentation>Код БKИ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="BankSign">
                <xs:annotation>
                    <xs:documentation>Наименование кредитора</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:int" name="nRelationshipDuration">
                <xs:annotation>
                    <xs:documentation>Длительность взаимоотношений с банком (дней)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:date" name="lastRecordUpdate">
                <xs:annotation>
                    <xs:documentation>Дата последнего обновления записи</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:int" name="liabilityStatus">
                <xs:annotation>
                    <xs:documentation>Статус обязательства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="LiabilityTypeDictType" name="liabilityType">
                <xs:annotation>
                    <xs:documentation>Вид обязательства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="LiabilityTypeDictType" name="baseLiabilityType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Вид основного обязательства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="amountInitial"  minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Начальная сумма обязательств</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="interestRate" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Процентная ставка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="residualDebt" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Общий остаток задолжности по основному долгу</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="liabilityCurrency" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Валюта, в которой указана сумма, в формате ISO, т.е. USD, EUR и т.д.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="liabilityNo" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Номер кредитного договора / банковской карты</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:date" name="dateStart">
                <xs:annotation>
                    <xs:documentation>Дата открытия кредитного договора</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:date" name="dateRedemptionScheduled">
                <xs:annotation>
                    <xs:documentation>Плановая дата окончания срока действия обязательства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:date" name="dateRedemptionActual" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Фактическая дата погашения обязательства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:int" name="repaymentMethod" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Метод погашения задолжности</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="currentArrearAmount" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Сумма текущей просроченной задолжности</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:int" name="isRestructured">
                <xs:annotation>
                    <xs:documentation>Наличие реструктуризации условий кредитования</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:decimal" name="cred_sum_limit" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Неиспользованный кредитный лимит</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:int" name="cred_prolong" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Количество пролонгаций</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="kodTb" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Код тербанка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="nameTb" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Наименование тербанка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:int" name="liabilityArrearsBlockCount">
                <xs:annotation>
                    <xs:documentation>Количество блоков с информацией о просрочках (за год)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="BlockDataASSDType" name="blockDataASSD" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Блок данных из АС СД</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="LiabilityArrearsBlockType" name="liabilityArrearsBlock" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Блок информации о просрочненных задолженностях по Договору, в разрезе каждого анализируемого года</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ResponseType">
        <xs:annotation>
            <xs:documentation>Ответ АС ОКИ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="1" maxOccurs="1" type="DealResultType" name="dealResult"/>
            <xs:element minOccurs="0" maxOccurs="1" type="ParticipantResultsType" name="participantResults"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DealResultType">
        <xs:annotation>
            <xs:documentation>Результат оценки по сделке</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="1" type="SystemErrorsType" name="systemErrors"/>
            <xs:element minOccurs="0" maxOccurs="unbounded" type="DealScoringResultType" name="dealScoringResult"/>
            <xs:element minOccurs="1" maxOccurs="1" nillable="false" type="ScoringStatusDictType" name="status">
                <xs:annotation>
                    <xs:documentation>Статус оценки по сделке</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="strategyCodes" type="xs:string" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Вектор с кодами ошибок, возвращаемых из стратегии</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="comments" type="xs:string" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Комментарий</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SystemErrorsType">
        <xs:annotation>
            <xs:documentation>Список ошибок обработки</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element type="SystemErrorType" name="error" maxOccurs="unbounded" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SystemErrorType">
        <xs:annotation>
            <xs:documentation>Ошибка обработки в системе АС ОКИ, список ошибок:
                SYSTEM-OKI-001  Неизвестная ошибка в системе
                SYSTEM-OKI-002  Время вызова БКИ превысило заданное
                SYSTEM-OKI-003  Ошибка вызова БД
                SYSTEM-OKI-004  Количество участников сделки превышает допустимое значение
                SYSTEM-OKI-005  Сделка не найдена
                SYSTEM-OKI-006  Сделка находится в неверном статусе, невозможно отправить на оценку
                SYSTEM-OKI-007  Ошибка трансформации в XML или из XML
                SYSTEM-OKI-008  Лимит сообщений в день превышен
                SYSTEM-OKI-009  Ошибка БКИ
                SYSTEM-OKI-010  Ошибка стратегии
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element type="xs:string" name="code" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Код ошибки</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="message" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Описание ошибки</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:string" name="cause" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Причина ошибки</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ParticipantResultsType">
        <xs:annotation>
            <xs:documentation>Список результатов оценки по участникам сделки</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="participantResult" type="ParticipantResultType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ParticipantScoringResultType">
        <xs:annotation>
            <xs:documentation>Результат оценки по участнику сделки</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="internalScoringGrade" type="xs:string" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Внутренняя оценка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="externalScoringGrade" type="xs:string" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Внешняя оценка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="unifiedScoringGrade" type="xs:string" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Совокупная оценка</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="strategyCodes" type="xs:string" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Коды ошибок</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="comments" type="xs:string" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Комментарий</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ParticipantResultType">
        <xs:annotation>
            <xs:documentation>Список результатов оценки по участнику сделки</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element type="xs:int" minOccurs="1" maxOccurs="1" nillable="false" name="participantIndex">
                <xs:annotation>
                    <xs:documentation>Уникальный индекс участника в сделке</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="1" maxOccurs="1" nillable="false" type="ScoringStatusDictType" name="status">
                <xs:annotation>
                    <xs:documentation>Статус результата оценки для участника сделки</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="ParticipantScoringResultType" minOccurs="0" maxOccurs="1" name="participantScoringResult"/>
            <xs:element type="ParticipantCreditHistoriesType" minOccurs="0" maxOccurs="1" name="histories"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="RequestType">
