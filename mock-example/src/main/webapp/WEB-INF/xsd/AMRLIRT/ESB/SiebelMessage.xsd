<xs:schema attributeFormDefault="unqualified"
	elementFormDefault="qualified" xmlns:altova="http://www.altova.com/xml-schema-extensions"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://service.lgd.sberbank.luxoft.com/" targetNamespace="http://service.lgd.sberbank.luxoft.com/">
	
<xs:complexType name="SiebelMessage">
		<xs:annotation>
			<xs:documentation>Заголовок запроса с обязательными данными
			</xs:documentation>
		</xs:annotation>
		
		<xs:all>
			<xs:element name="ListOfAccRoot">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AccRoot" type="AccRoot" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
		
		<xs:attribute name="MessageId" type="xs:string" use="required">
			<xs:annotation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="1-2KHH9" />
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IntegrationType" type="xs:string" use="required">
			<xs:annotation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="LGD" />
					</altova:exampleValues>
				</xs:appinfo>
				<xs:documentation>Тип точки интеграции</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TemplateId" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Идентификатор шаблона, на основании котогоро
					собрано сообщение
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TestRequest" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Идентификатор шаблона, на основании котогоро
					собрано сообщение
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Version" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Идентификатор шаблона, на основании котогоро
					собрано сообщение
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
</xs:complexType>
	<xs:simpleType name="SiebelNumber">
		<xs:annotation>
			<xs:documentation>Формат числовых значений</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="-100000,12" />
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(-?\d+(,\d+)?)?" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SiebelDate">
		<xs:annotation>
			<xs:documentation>Формат даты</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="10.02.2012" />
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{2}\.\d{2}\.\d{4}" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SiebelDateTime">
		<xs:annotation>
			<xs:documentation>Формат даты-времени</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="10.02.2012 12:10:32" />
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{2}\.\d{2}\.\d{4} \d{2}:\d{2}:\d{2}" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SiebelBoolean">
		<xs:annotation>
			<xs:documentation>Формат булевых значений</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Y" />
			<xs:enumeration value="N" />
		</xs:restriction>
	</xs:simpleType>

<xs:complexType name="AccRoot">
	<xs:annotation>
		<xs:documentation>Организации</xs:documentation>
	</xs:annotation>
	<xs:all>
	
		<xs:element minOccurs="0" name="Type">
				<xs:annotation>
					<xs:documentation>Тип карточки</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		<xs:element minOccurs="0" name="Name">
				<xs:annotation>
					<xs:documentation>Полное наименование организации</xs:documentation>
				</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="100" />
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
		<xs:element minOccurs="0" name="SBRFINN">
			<xs:annotation>
				<xs:documentation>ИНН</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="15" />
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
			
		<xs:element minOccurs="0" name="ListOfAccRootAccCountryReg">
			<xs:annotation>
				<xs:documentation>Список стран, где организация зарегистрирована
				</xs:documentation>
			</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0"
							name="AccRootAccCountryReg" type="AccRootAccCountryReg" />
					</xs:sequence>
				</xs:complexType>
		</xs:element>
		<xs:element minOccurs="0" name="ListOfRequest">
		<xs:annotation>
			<xs:documentation>Сделки (Сделка КК , Сделка ОПК, Сделка СМО)
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="unbounded" minOccurs="0" name="Request"
					type="Request" />
				</xs:sequence>
		</xs:complexType>
		</xs:element>
		<xs:element minOccurs="0" name="ListOfAccServAgr">
			<xs:annotation>
				<xs:documentation>Кредитные договоры</xs:documentation>
			</xs:annotation>
			<xs:complexType>
				<xs:sequence>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
		<xs:element minOccurs="0" name="ListofRating">
			<xs:annotation>
				<xs:documentation>Рейтинги</xs:documentation>
			</xs:annotation>
			<xs:complexType>
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="0"
						name="Rating" type="Rating" />
				</xs:sequence>
			</xs:complexType>
		</xs:element>
		
	</xs:all>
	<xs:attribute name="Current" type="SiebelBoolean" use="required">
			<xs:annotation>
				<xs:documentation>Флаг, означающий, что данная запись была выбрана в
					интерфейсе при вызове интеграции
				</xs:documentation>
			</xs:annotation>			
		</xs:attribute>

</xs:complexType>

<xs:complexType name="AccRootAccCountryReg">
	<xs:annotation>
		<xs:documentation>Страна регистрации организации</xs:documentation>
	</xs:annotation>
	<xs:all>
			<xs:element name="Id">
				<xs:annotation>
					<xs:documentation>Идентификатор записи в CRM</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Name">
				<xs:annotation>
					<xs:documentation>Название страны</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1024" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SBRFCountryLiteralCode">
				<xs:annotation>
					<xs:documentation>3-символьный код страны</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
	</xs:all>

	<xs:attribute name="Current" type="SiebelBoolean" use="required">
			<xs:annotation>
				<xs:documentation>Флаг, означающий, что данная запись была выбрана в
					интерфейсе при вызове интеграции
				</xs:documentation>
			</xs:annotation>			
		</xs:attribute>
</xs:complexType>
	
