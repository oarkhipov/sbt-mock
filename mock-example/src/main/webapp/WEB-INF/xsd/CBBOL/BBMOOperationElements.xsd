	<element name="SrvPutRemoteLegalAccOperAppRq">
		<annotation>
			<documentation>Запрос на создание задачи на открытие счета по запросу СББОЛ</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="ct:DataProcessingRequest">
					<sequence>
						<element name="RemoteClient" type="dt:Boolean" minOccurs="0">
					        <annotation>
						        <documentation>Признак удалённого создания клиента</documentation>
					        </annotation>
				        </element>
				        <element name="RemoteSystemId" type="dt:String" minOccurs="0">
					        <annotation>
						        <documentation>Система источник запрос в ЕКС</documentation>
					        </annotation>
				        </element>
