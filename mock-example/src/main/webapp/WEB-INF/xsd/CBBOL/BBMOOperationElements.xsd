	<element name="SrvPermitAccountChargeOperationRq">
		<annotation>
			<documentation>Запрос на разрешение проведения расходных операций</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="ct:DataProcessingRequest">
					<sequence>
						<element ref="tns:BankSubdivision">
							<annotation>
								<documentation>Подразделение
Справочник подразделений Банка</documentation>
							</annotation>
						</element>
						<element name="AccountNumber">
							<annotation>
								<documentation>
                                    Номер расчетного счета. Опционально (через символ "|") указывается тип операции:
                                        0-заблокировать,
                                        1-разблокировать.
                                </documentation>
							</annotation>
							<simpleType>
								<restriction base="dt:String">
									<pattern value=".{20}(\|[01]{1}|.{0})"/>
								</restriction>
							</simpleType>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
