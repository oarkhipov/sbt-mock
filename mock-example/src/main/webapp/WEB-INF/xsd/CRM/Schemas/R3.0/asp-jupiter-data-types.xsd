   </xsd:annotation>
   </xsd:element>
   <!-- Выбранные подвиды вклада-->
   <xsd:element name="subDepositUsed" type="tns:ReferenceDepositSubTypes" minOccurs="0" maxOccurs="unbounded">
   <xsd:annotation>
   <xsd:documentation>Выбранные подвиды вклада</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Идентификация договора в CRM-->
   <xsd:element name="CRMID" type="sa:IDType" minOccurs="0">
