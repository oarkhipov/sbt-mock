    <xsd:documentation>Тариф за последующие  года (ЮЛ)</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Тариф за 1 год обслуживания (ФЛ)-->
   <xsd:element name="firstYearFIZ" type="xsd:decimal" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Тариф за 1 год обслуживания (ФЛ)</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Тариф за последующие  года (ФЛ)-->
