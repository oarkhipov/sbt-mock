   <xsd:documentation>Пакет услуг</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Тариф за обслуживание-->
   <xsd:element name="paymentTariffs" type="tns:ServiceTariff" minOccurs="0" maxOccurs="unbounded">
   <xsd:annotation>
   <xsd:documentation>Тариф за обслуживание</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Используемые виды зачислений-->
   <xsd:element name="paymentTypes" type="tns:PaymentUsed" minOccurs="0" maxOccurs="unbounded">
