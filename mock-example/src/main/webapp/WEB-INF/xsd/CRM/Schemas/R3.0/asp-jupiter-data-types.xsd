    <xsd:documentation>Тариф за 1 год обслуживания (ЮЛ)</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Тариф за последующие  года (ЮЛ)-->
   <xsd:element name="nextYearUL" type="xsd:decimal" minOccurs="0">
