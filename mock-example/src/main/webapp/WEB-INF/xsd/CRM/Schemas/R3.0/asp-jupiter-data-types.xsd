   </xsd:annotation>
   </xsd:element>
   <!-- Порядок уплаты комиссии-->
   <xsd:element name="commissionOrder" type="tns:ReferenceCommissionOrder" minOccurs="0">
   <xsd:annotation>
   <xsd:documentation>Порядок уплаты комиссии</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Периодичность расчета коммиссии-->
   <xsd:element name="commissionPeriod" type="tns:ReferenceCommissionPeriodical" minOccurs="0">
   <xsd:annotation>
   <xsd:documentation>Периодичность расчета коммиссии</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Валюта-->
   <xsd:element name="currency" type="tns:ReferenceCurrency" minOccurs="0">
   <xsd:annotation>
   <xsd:documentation>Валюта</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Пакет услуг-->
   <xsd:element name="packageOfService" type="tns:PackageOfService" minOccurs="0">
