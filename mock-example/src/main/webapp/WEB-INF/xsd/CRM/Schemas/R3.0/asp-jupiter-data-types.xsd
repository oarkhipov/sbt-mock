  <xsd:complexType name="SalaryAgreementType">
   <xsd:annotation>
   <xsd:documentation>Зарплатный договор</xsd:documentation>
   </xsd:annotation>
   <xsd:sequence>
   <!-- Номер договора-->
   <xsd:element name="agreementNumber" type="dt:DefString" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Номер ЗД</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Версия ЗД-->
   <xsd:element name="agreementVersion" type="dt:String" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Версия ЗД</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Процесс-->
   <xsd:element name="processType" type="xsd:string" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Процесс</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Номер ЗД предшественника-->
   <xsd:element name="previousAgreementNumber" type="dt:DefString" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Если ЗД создан в процессе перезаключения</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Дата регистрации-->
   <xsd:element name="registrationDate" type="xsd:date" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Дата регистрации</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Дата создания-->
   <xsd:element name="creationDate" type="xsd:date" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Дата создания</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Дата начала действия договора-->
   <xsd:element name="fromDate" type="xsd:date">
    <xsd:annotation>
    <xsd:documentation>Дата начала действия договора</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Дата окончания договора-->
   <xsd:element name="toDate" type="xsd:date" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Дата окончания договора</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Единый централизованный договор-->
   <xsd:element name="isCentralAgreement" type="xsd:boolean" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Единый централизованный договор: false - не выбрано значение; true- выбрано
    </xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Оферта-->
   <xsd:element name="isOffer" type="xsd:boolean" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Признак оферты</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Тарифный план-->
   <xsd:element name="isTariffPlan" type="xsd:boolean" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Признак наличия тарифного плана</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Плата за зачисления на счета в своем филиале-->
   <xsd:element name="commissionForServiceInOwnBranch" type="xsd:decimal" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Размер платы за услуги банка по зачислению в своем филиале</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Плата за зачисления на счета в других филиалах-->
   <xsd:element name="commissionForServiceInBankBranch" type="xsd:decimal" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Размер платы за услуги банка по зачислению в филиалах банка*</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Признак оформления отдельных реестров-->
   <xsd:element name="isSeparateRegister" type="xsd:boolean" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Признак оформления отдельных реестров на резидентов и нерезидентов</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Номер расчетного счета-->
   <xsd:element name="accountOfPayment" type="dt:DefString" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Номер расчетного счета организации, с которого Банку согласно условиям договора разрешается формировать платежное поручение на сумму поступившего от организации реестра (без резервирования)</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Номер лицевого счета-->
   <xsd:element name="accountOfClient" type="dt:DefString" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Номер лицевого счета, открытого в ГРЦ/ТРЦ/ГОСБ для организации зачислений</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Номер расчетного счета организации (счет Ф-363)-->
   <xsd:element name="commissionAccount" type="dt:DefString" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Номер расчетного счета организации, к которому банк формирует счет ф 363 на оплату комиссии</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Количество рабочих дней Ф-363-->
   <xsd:element name="workingDaysOf363F" type="xsd:int" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Количество рабочих дней для оплаты организацией счета ф-363
    для случая когда РКО не в СБ. 
    Передавать в АС ВИК</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- План. сумма зачислений в мес.-->
   <xsd:element name="fot" type="xsd:decimal" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>План. сумма зачислений в месяц (руб)</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- План. кол-во получателей в мес-->
   <xsd:element name="employeeForEmission" type="xsd:int" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Плановое кол-во получателей</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Кол-во зарплатных карт-->
   <xsd:element name="numberOfCards" type="xsd:int" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Общая численность зарплатных карт</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Срок возврата средств получателям по результатам проведенного зачисления-->
   <xsd:element name="periodOfRepaymentInterest" type="xsd:int" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Срок возврата средств получателям по результатам проведенных зачислений</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Срок возврата средств организации, если операция по зачислению не совершалась-->
   <xsd:element name="periodOfRepaymentFunds" type="xsd:int" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Уточнть значения (1-7)
    Срок возврата средств без зачислений</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Плата за пересылку реестров в другие филиалы-->
   <xsd:element name="commissionForTransferRegister" type="xsd:decimal" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Размер платы за услуги банка по пересылке реестров</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Комментарий ССЗП-->
   <xsd:element name="commentSAManager" type="dt:String" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Комментарий ССЗП</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Комментарий КЮЛ-->
   <xsd:element name="commentKUL" type="dt:String" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Комментарий КЮЛ</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Комментарий КЗД-->
   <xsd:element name="commentKZD" type="dt:String" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Комментарий КЗД</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Количество подписей по ЗД-->
   <xsd:element name="numberOfSigners" type="xsd:int" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Кол-во зарплатных карт</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Признак кураторства ЦА-->
   <xsd:element name="isSuperviserCA" type="xsd:boolean" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Признак кураторства ЦА</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Счета к ДС в свой филиал-->
   <xsd:element name="accountsInOwnBranch" type="dt:String" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Счета к доп.соглашению на списание Банком на основании реестра с расч.счета в своем филиале фактически зачисленной суммы на счета физ.лиц, ведущихся в этом же (одном) филиале Банка</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Счета к ДС в другой филиал-->
   <xsd:element name="accountsInBankBranch" type="dt:String" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Счета к доп.соглашению на списание Банком на основании электр.реестра с расч.счета в своем филиале суммы на зачисление на счета физ.лиц в другом филиале и суммы платы за зачисл.,пересылку реестров</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Признак счетов на РКО-->
   <xsd:element name="isRKO" type="xsd:boolean" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Признак счетов на РКО</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Статус в CRM-->
   <xsd:element name="statusCRM" type="dt:DefString" minOccurs="0">
