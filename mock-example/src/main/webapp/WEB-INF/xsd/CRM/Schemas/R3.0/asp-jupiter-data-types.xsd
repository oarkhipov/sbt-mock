   <xsd:element name="clientInfo" type="tns:Client" minOccurs="0">
   <xsd:annotation>
   <xsd:documentation>Информация о клиенте</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Сотрудники привлечения-->
   <xsd:element name="managers" type="tns:Employee" minOccurs="0" maxOccurs="unbounded">
   <xsd:annotation>
   <xsd:documentation>Сотрудники привлечения</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Куратор ЦА-->
