   <xsd:element name="rejectReasons" type="tns:RejectReason" minOccurs="0" maxOccurs="unbounded">
   <xsd:annotation>
   <xsd:documentation>Причины отказа</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
