    </xsd:annotation>
   </xsd:element>
   <!-- Дата в CRM-->
   <xsd:element name="dateCRM" type="xsd:date" minOccurs="0">
    <xsd:annotation>
    <xsd:documentation>Дата в CRM: После интеграции с CRM</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <!-- Счет-->
   <xsd:element name="accounts" type="tns:AccountType" minOccurs="0" maxOccurs="unbounded">
   <xsd:annotation>
   <xsd:documentation>Счет</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- -->
   <xsd:element name="agreementStatus" type="tns:ReferenceContractStatus"/>
   <!-- Тип договора-->
   <xsd:element name="agreementType" type="tns:ReferenceContractType" minOccurs="0">
   <xsd:annotation>
   <xsd:documentation>Тип договора</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Реквизиты банка-->
   <xsd:element name="bankRequisite" type="tns:BankRequisite" minOccurs="0">
   <xsd:annotation>
   <xsd:documentation>Реквизиты банка</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Вид расчетов-->
   <xsd:element name="calculationType" type="tns:ReferenceCalculation" minOccurs="0">
   <xsd:annotation>
   <xsd:documentation>Вид расчетов</xsd:documentation>
   </xsd:annotation>
   </xsd:element>
   <!-- Реквизиты клиента-->
   <xsd:element name="clientRequisite" type="tns:ClientRequisite" minOccurs="0" maxOccurs="unbounded">
   <xsd:annotation>
