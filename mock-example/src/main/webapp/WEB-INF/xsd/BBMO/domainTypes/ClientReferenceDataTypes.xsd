						<documentation>Остаток по счету</documentation>
					</annotation>
				</element>
				<element name="FilePresence1" type="dt:Boolean" minOccurs="0">
					<annotation>
						<documentation>Наличие картотеки 1 (для договора РКО в рублях)</documentation>
					</annotation>
				</element>
				<element name="FilePresence2" type="dt:Boolean" minOccurs="0">
					<annotation>
						<documentation>Наличие картотеки 2 (для договора РКО в рублях)
                        </documentation>
					</annotation>
				</element>
				<element name="ArrestByAccountPresence" minOccurs="0">
