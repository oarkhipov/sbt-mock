	<element name="SrvUpdateClientReferenceDataRq">
		<annotation>
			<documentation>Запрос на изменение справочных данных клиента</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="ct:DataProcessingRequest">
					<sequence>
						<element name="UpdateType">
							<annotation>
								<documentation>Тип загрузки
1 – Загрузить 
2 – Обновить 
3 – Изменить
Зарезервировано. В данной версии не используется.</documentation>
							</annotation>
							<simpleType>
								<restriction base="dt:DefPosInteger">
									<enumeration value="1"/>
									<enumeration value="2"/>
									<enumeration value="3"/>
								</restriction>
							</simpleType>
						</element>
						<element name="ChangeType">
							<annotation>
								<documentation>Тип изменения
1 – Изменение данных клиента
2 – Внесение измене-ний в юр. дело</documentation>
							</annotation>
							<simpleType>
								<restriction base="dt:DefPosInteger">
									<enumeration value="1"/>
									<enumeration value="2"/>
								</restriction>
							</simpleType>
						</element>
						<element name="ChangeSubject" type="dt:DefString" minOccurs="0">
							<annotation>
								<documentation>Содержание изменения</documentation>
							</annotation>
						</element>
						<element name="VSPCommissionPayment" type="dt:Boolean" minOccurs="0">
							<annotation>
								<documentation>Признак взятия комиссии в ВСП</documentation>
							</annotation>
						</element>
						<element ref="tns:ClientReferenceData">
							<annotation>
								<documentation>Справочные данные клиента</documentation>
							</annotation>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
