	<element name="SrvGetExchangeControlDocumentInformationRq">
		<annotation>
			<documentation>Запрос на получение информации по документам валютного контроля</documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="ct:DataProcessingRequest">
					<sequence>
						<element name="CustomerId" type="dt:PosInteger" minOccurs="0">
							<annotation>
								<documentation>ID клиента (Передается в случае отсутствия информации о ИНН и КПП)</documentation>
							</annotation>
						</element>
						<element ref="tns:BankSubdivision" minOccurs="0">
							<annotation>
								<documentation>Подразделение банка (Передается в случае отсутствия информации о ИНН и КПП)</documentation>
							</annotation>
						</element>
						<element name="TaxId" type="st:TaxId_Type" minOccurs="0">
							<annotation>
								<documentation>ИНН клиента</documentation>
							</annotation>
						</element>
						<element name="TaxRegReasonCode" type="st:TaxRegReasonCode_Type" minOccurs="0">
							<annotation>
								<documentation>КПП клиента</documentation>
							</annotation>
						</element>
						<element name="DocumentKind">
							<annotation>
								<documentation>Вид документа
Принимает значения:
1 – Паспорт сделки
2 – СоВО с ПС
3 – СоВО без ПС
4 – СоПВРФ
5 – СоПД
6 – СоРЧСЗР
7 – ЕГРЮЛ</documentation>
							</annotation>
							<simpleType>
								<restriction base="dt:DefPosInteger">
									<enumeration value="1"/>
									<enumeration value="2"/>
									<enumeration value="3"/>
									<enumeration value="4"/>
									<enumeration value="5"/>
									<enumeration value="6"/>
									<enumeration value="7"/>
								</restriction>
							</simpleType>
						</element>
						<element name="TransPassportNum" type="st:TransPassportNum_Type" minOccurs="0">
							<annotation>
								<documentation>Номер паспорта сделки</documentation>
							</annotation>
						</element>
						<element name="AccountNumber" type="st:AccountNumber_Type" minOccurs="0">
							<annotation>
								<documentation>Номер счета</documentation>
							</annotation>
						</element>
						<element name="CurrencyCode" type="st:CurrencyCode_Type" minOccurs="0">
							<annotation>
								<documentation>Код валюты счета</documentation>
							</annotation>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
